{
    "manifest_version": 3,
  
    "name": "Carrot",
    "description": "Rabbit hole visualization",
    "version": "1.0",
    "permissions": ["storage", "tabs", "tabGroups"],
    "host_permissions": [
      "https://developer.chrome.com/*"
    ],
    
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "/images/carrot.png",
        "48":  "/images/carrot.png",
        "128":  "/images/carrot.png"

      }
    },

    "icons": {
        "16": "/images/carrot.png",
        "48": "/images/carrot.png",
        "128": "/images/carrot.png"
    },

    "background": {
      "service_worker": "popup/popup.js"
    }

  }